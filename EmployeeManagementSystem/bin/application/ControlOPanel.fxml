<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.ControlPanel">
   <children>
      <Pane onDragDetected="#viewEmployee" prefHeight="380.0" prefWidth="529.0">
         <children>
            <Button layoutX="34.0" layoutY="109.0" mnemonicParsing="false" onAction="#viewEmployee" text="View Employee" />
            <Button layoutX="34.0" layoutY="178.0" mnemonicParsing="false" onAction="#UpdateDelete" text="Update / Delete" />
            <Label layoutX="225.0" layoutY="44.0" prefHeight="17.0" prefWidth="79.0" text="Control Panel" />
            <Button layoutX="34.0" layoutY="252.0" mnemonicParsing="false" onAction="#InsertEmployee" text="Insert Employee" />
            <ImageView fitHeight="257.0" fitWidth="206.0" layoutX="296.0" layoutY="123.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@image/Employee.jpg" />
               </image>
            </ImageView>
         </children>
      </Pane>
   </children>
</AnchorPane>
